# =============================================================================
# MedAssist Healthcare Platform - Environment Configuration Example
# =============================================================================
# Copy this file to .env and fill in your actual API keys and configuration

# =============================================================================
# REQUIRED - SUPABASE CONFIGURATION
# =============================================================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# =============================================================================
# REQUIRED - AI SERVICES
# =============================================================================
VITE_OPENAI_API_KEY=sk-your_openai_api_key
VITE_GOOGLE_CLOUD_API_KEY=your_google_cloud_api_key
VITE_VISION_API_KEY=your_vision_api_key

# =============================================================================
# REQUIRED - VIDEO CONSULTATION
# =============================================================================
VITE_AGORA_APP_ID=your_agora_app_id

# =============================================================================
# OPTIONAL - ADDITIONAL SERVICES
# =============================================================================
VITE_TWILIO_ACCOUNT_SID=your_twilio_account_sid
VITE_TWILIO_AUTH_TOKEN=your_twilio_auth_token
VITE_SENDGRID_API_KEY=your_sendgrid_api_key
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================
VITE_NODE_ENV=development
VITE_DEBUG_MODE=true
VITE_API_BASE_URL=http://localhost:3000